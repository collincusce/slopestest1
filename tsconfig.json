{
    "compilerOptions": {
      "outDir": "./dist",
      "module": "commonjs",
      "target": "ES2015",
      "lib": [
        "dom",
        "es2016"
      ],
      "declaration": true,
      "resolveJsonModule": true,
      "skipLibCheck": true,
      "inlineSourceMap": true,
      "inlineSources": true,
      "declarationMap": true,
      "noFallthroughCasesInSwitch": true,
      "strictFunctionTypes": true,
      "esModuleInterop": true
    },
    "exclude": [
      "node_modules"
    ],
    "typeAcquisition": {
      "enable": true
    }
  }
